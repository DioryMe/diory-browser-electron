{
  "name": "diory-browser-electron",
  "version": "5.0.0",
  "productName": "Diory",
  "main": "electron-main.js",
  "private": true,
  "dependencies": {
    "dayjs": "^1.10.7",
    "electron-devtools-installer": "^3.2.0",
    "electron-notarize": "^1.1.1",
    "electron-reload": "^2.0.0-alpha.1",
    "electron-store": "^8.0.1",
    "exifreader": "^4.1.0",
    "file-type": "^16.4.0",
    "glob": "^7.2.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@4tw/cypress-drag-drop": "^2.1.0",
    "@babel/core": "^7.16.5",
    "@babel/plugin-transform-runtime": "^7.16.5",
    "@babel/polyfill": "^7.10.1",
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-react": "^7.16.5",
    "@babel/register": "^7.16.5",
    "@storybook/addon-actions": "^6.4.9",
    "@storybook/addon-links": "^6.4.9",
    "@storybook/addons": "^6.4.9",
    "@storybook/preset-create-react-app": "^4.0.0",
    "@storybook/react": "^6.4.9",
    "@types/jest": "^27.0.3",
    "babel-loader": "^8.2.3",
    "cross-env": "^7.0.3",
    "cypress": "9.2.0",
    "cypress-cucumber-preprocessor": "^4.3.1",
    "cypress-react-selector": "^2.3.13",
    "electron": "^16.0.5",
    "eslint": "^8.5.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^25.3.2",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "evergreen-ui": "6.6.4",
    "husky": "^7.0.4",
    "leaflet": "^1.6.0",
    "lint-staged": "^12.1.4",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "prop-types": "^15.8.0",
    "react": "^17.0.2",
    "react-dnd": "^14.0.4",
    "react-dnd-html5-backend": "^14.0.2",
    "react-dom": "^17.0.2",
    "react-pdf": "^5.5.0",
    "react-redux": "^7.2.6",
    "react-scripts": "4.0.3",
    "redux": "^4.1.2",
    "testcafe": "^1.18.1",
    "testcafe-browser-provider-electron": "^0.0.17",
    "ui-box": "^5.0.0",
    "wait-on": "^6.0.0"
  },
  "optionalDependencies": {
    "electron-builder": "^22.14.5"
  },
  "homepage": "./",
  "scripts": {
    "start": "BROWSER=none PORT=3300 ESLINT_NO_DEV_ERRORS=true react-scripts start",
    "start-electron": "export ELECTRON_START_URL=http://localhost:3300 && electron .",
    "start-electron-dev-tools": "DEV_TOOLS=true export ELECTRON_START_URL=http://localhost:3300 && cross-env nodemon --exec electron .",
    "build-and-start-electron": "yarn build && electron .",
    "build": "react-scripts build",
    "build-electron": "mkdir build/src && cp electron-main.js build && cp -r electron/. build/electron && cp -r src/shared/. build/src/shared",
    "package": "electron-builder build -c.extraMetadata.main=build/electron-main.js --publish never",
    "package-mac": "yarn package --mac --config electron-builder.json",
    "package-mac-rc": "yarn package --mac --config electron-builder-rc.json",
    "package-linux": "yarn package --linux",
    "package-win": "yarn package --win",
    "eslint": "eslint './*.{js, json}' './{src,electron,scripts}/**/*.{js,jsx,json,css,scss}' --fix",
    "prettier": "prettier --write '{electron,src}/**/*.{js,jsx,json,css,scss}'",
    "test": "yarn test-electron && yarn prettier && yarn eslint && yarn test-react",
    "test-react": "react-scripts test --verbose",
    "test-electron": "jest electron/**/*.spec.js --setupFilesAfterEnv ./src/setupTests.js --detectOpenHandles --roots electron --verbose",
    "test-cypress": "yarn cypress run",
    "test-cypress-open": "yarn cypress open",
    "storybook": "ESLINT_NO_DEV_ERRORS=true start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "eject": "react-scripts eject",
    "prepare": "husky install"
  },
  "build": {
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "publish": {
      "provider": "github",
      "repo": "diory-browser-electron",
      "owner": "dioryme"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime"
    ]
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!(react-pdf/dist/esm/entry.webpack)/)"
    ],
    "moduleNameMapper": {
      "react-pdf/dist/esm/entry.webpack": "react-pdf"
    }
  },
  "prettier": {
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "printWidth": 100
  },
  "nodemonConfig": {
    "ignore": [
      "public/*",
      "docs/*"
    ],
    "delay": 2000
  },
  "lint-staged": {
    "{electron,src,scripts}/**/*.{js,jsx,json,css,scss}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": false,
    "stepDefinitions": "./src/react/utils/cypress/step-definitions"
  }
}
